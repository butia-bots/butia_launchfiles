<launch>
    <include file="$(find butia_navigation_launchfiles)/navigate-doris-full.launch"/>
    <machine
        name="jetson"
        address="jetson"
        env-loader="/opt/ros/melodic/env_jetson.sh"
        default="false"
        user="jetson"
    />
    <include file="$(find butia_vision_bridge)/launch/butia_vision_bridge.launch">
        <arg name="machine" value="jetson"/>
    </include>
</launch>